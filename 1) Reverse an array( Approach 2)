#include <iostream>
#include <stack>
using namespace std;

int main() {
	stack<int> rev;
	int t,n,k;
	cin >> t;
	while(t--){
	    cin >> n;
	    for(int i=0;i<n;i++){
	        cin >> k;
	        rev.push(k);
	    }
	    while(!rev.empty()){
	        cout << rev.top() << " ";
	        rev.pop();
	    }
	}
	return 0;
}
