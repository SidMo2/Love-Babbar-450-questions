// { Driver Code Starts
#include <bits/stdc++.h>
using namespace std;

 // } Driver Code Ends
//User function template for C++
class Solution{
public:
	int rowWithMax1s(vector<vector<int> > arr, int n, int m) {
	    // code here
	    int max_count=0,pos=0,c=0;
	    for(int i=0;i<n;i++){
	        c=count(arr[i].begin(),arr[i].end(),1);
	        if(max_count<c){
	            max_count=max(max_count,c);
	            pos=i;
	        }
	    }
	    if(max_count==0){
	        return -1;
	    }
	    else{
	        return pos;
	    }
	}

};

// { Driver Code Starts.
int main() {
    int t;
    cin >> t;
    while (t--) {
        int n, m;
        cin >> n >> m;
        vector< vector<int> > arr(n,vector<int>(m));
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < m; j++) {
                cin>>arr[i][j];
            }
        }
        Solution ob;
        auto ans = ob.rowWithMax1s(arr, n, m);
        cout << ans << "\n";
    }
    return 0;
}
  // } Driver Code Ends
