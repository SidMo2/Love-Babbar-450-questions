//Initial template for C++

#include<bits/stdc++.h>
using namespace std;


 // } Driver Code Ends
//User function template for C++

class Solution{
  public:
     // Function to find majority element in the array
    // a: input array
    // size: size of input array
    int majorityElement(int a[], int size)
    {
        
        // your code here
    map<int, int> countmap;
    for(int i=0;i<size;i++){
        if(countmap.find(a[i])==countmap.end()){
            countmap[a[i]]=1;
        }
        else{
            countmap[a[i]]++;
        }
    }
    int max_count=-1;
    int max_val=0;
    for(auto itr=countmap.begin();itr!=countmap.end();itr++){
        if(itr->second>max_count){
            max_count=itr->second;
            max_val=itr->first;
        }
    }
    if(max_count>size/2){
        return max_val;
    }
    else{
        return -1;
    }
    }
};

// { Driver Code Starts.

int main(){

    int t;
    cin >> t;

    while(t--){
        int n;
        cin >> n;
        int arr[n];
        
        for(int i = 0;i<n;i++){
            cin >> arr[i];
        }
        Solution obj;
        cout << obj.majorityElement(arr, n) << endl;
    }

    return 0;
}
  // } Driver Code Ends
